{
    "name": "userfrosting/theme-uikit",
    "type": "project",
    "description": "UiKit based theme for UserFrosting, using Vue.js",
    "keywords": ["Vue.js", "UiKit", "UserFrosting"],
    "homepage": "https://github.com/userfrosting/theme-uikit",
    "license" : "MIT",
    "authors" : [
        {
            "name": "Louis Charette",
            "homepage": "https://bbqsoftwares.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-gd": "*",
        "userfrosting/framework": "^5.0",
        "userfrosting/sprinkle-core": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.2",
        "league/factory-muffin": "^3.0",
        "league/factory-muffin-faker": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "UserFrosting\\Theme\\UiKit\\": "app/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserFrosting\\Theme\\UiKit\\App\\": "app/tests/"
        }
    }
}
